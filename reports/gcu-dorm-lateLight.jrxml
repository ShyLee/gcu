	<parameter name="dateEndValue" class="java.lang.String"/>
	<queryString>
		<![CDATA[select dv_id,dv_name,
       countLate1,countLight1,
       round(late1/$P{days},2) late1,
       round(light1/$P{days},2) light1,

       countLate2,countLight2,
       round(late2/$P{days},2) late2,
       round(light2/$P{days},2) light2,

       countLate3,countLight3,
       round(late3/$P{days},2) late3,
       round(light3/$P{days},2) light3,

       countLate4,countLight4,
       round(late4/$P{days},2) late4,
       round(light4/$P{days},2) light4,

       dvLate,dvLight,dvRm,dvStu,

       case when dvStu=0 then 0 else round(dvLate/dvStu/$P{days}*100,2) end late,
       case when dvRm=0 then 0 else round(dvLight/dvRm/$P{days}*100,2) end light

 from (
       select dv_id,dv_name,
       countLate1,countLight1,
       case when countStu1=0 then 0.00 else countLate1/countStu1*100 end late1,
       case when countRm1=0 then 0.00 else countLight1/countRm1*100 end light1,

       countLate2,countLight2,
       case when countStu2=0 then 0.00 else countLate2/countStu2*100 end late2,
       case when countRm2=0 then 0.00 else countLight2/countRm2*100 end light2,

       countLate3,countLight3,
       case when countStu3=0 then 0.00 else countLate3/countStu3*100 end late3,
       case when countRm3=0 then 0.00 else countLight3/countRm3*100 end light3,

       countLate4,countLight4,
       case when countStu4=0 then 0.00 else countLate4/countStu4*100 end late4,
       case when countRm4=0 then 0.00 else countLight4/countRm4*100 end light4,

       countLate1+countLate2+countLate4+countLate4 dvLate,
       countLight1+countLight2+countLight3+countLight4 dvLight,

       countRm1+countRm2+countRm3+countRm4 dvRm,
       countStu1+countStu2+countStu3+countStu4 dvStu


       from (
           select dv_id,dv_name,
           sum(case stu_in_year when $P{year1} then count_stu else 0 end) countStu1,
           sum(case stu_in_year when $P{year1} then count_rm else 0 end) countRm1,
           sum(case stu_in_year when $P{year1} then count_late else 0 end) countLate1,
           sum(case stu_in_year when $P{year1} then count_light else 0 end) countLight1,

           sum(case stu_in_year when $P{year2} then count_stu else 0 end) countStu2,
           sum(case stu_in_year when $P{year2} then count_rm else 0 end) countRm2,
           sum(case stu_in_year when $P{year2} then count_late else 0 end) countLate2,
           sum(case stu_in_year when $P{year2} then count_light else 0 end) countLight2,

           sum(case stu_in_year when $P{year3} then count_stu else 0 end) countStu3,
           sum(case stu_in_year when $P{year3} then count_rm else 0 end) countRm3,
           sum(case stu_in_year when $P{year3} then count_late else 0 end) countLate3,
           sum(case stu_in_year when $P{year3} then count_light else 0 end) countLight3,

           sum(case stu_in_year when $P{year4} then count_stu else 0 end) countStu4,
           sum(case stu_in_year when $P{year4} then count_rm else 0 end) countRm4,
           sum(case stu_in_year when $P{year4} then count_late else 0 end) countLate4,
           sum(case stu_in_year when $P{year4} then count_light else 0 end) countLight4
           from (
               select a.dv_id,a.dv_name,a.stu_in_year,a.count_stu,d.count_rm,b.count_late,c.count_light from
                 ( select b.dv_id, b.dv_name, b.stu_in_year, count_stu
                       from (select dv_id, stu_in_year, count(stu_no) count_stu
                             from sc_student
                            group by dv_id, stu_in_year
                            order by dv_id, stu_in_year) a,
                          sc_stu_dv_year b
                    where b.dv_id = a.dv_id(+)
                         and b.stu_in_year = a.stu_in_year(+)
                    order by b.dv_id, b.dv_name, b.stu_in_year) a,

                   --全校各个单位单个年级的晚归人数
                  (select b.dv_id,b.dv_name,b.stu_in_year,nvl(count_late,0) count_late
                       from (select stu_in_year, dv_id,
                                    sum(case disp_detail when '晚归' then 1 else 0 end) count_late
                                    from sc_stu_disp_log
                                    where disp_detail in('晚归')
                                          and date_disp >= to_date($P{dateStart}, 'yyyy/MM/dd')
                                          and date_disp <= to_date($P{dateEnd}, 'yyyy/MM/dd')
                                    group by stu_in_year, dv_id
                                    order by stu_in_year) a,
                       sc_stu_dv_year b
                       where b.dv_id=a.dv_id(+)
                             and b.stu_in_year=a.stu_in_year(+)
                       order by b.dv_id, b.dv_name, b.stu_in_year) b ,

                   --全校各个单位各个年级不熄灯数
                  ( select b.dv_id, b.dv_name, b.stu_in_year, nvl(count_light, 0) count_light
                     from (select stu_in_year, dv_id, count(*) count_light
                             from sc_stu_disp_log
                            where stu_no is null
                                  and date_disp >= to_date($P{dateStart}, 'yyyy/MM/dd')
                                  and date_disp <= to_date($P{dateEnd}, 'yyyy/MM/dd')
                            group by stu_in_year, dv_id) a,
                          sc_stu_dv_year b
                    where b.dv_id = a.dv_id(+)
                      and b.stu_in_year = a.stu_in_year(+)
                    order by b.dv_id, b.dv_name, b.stu_in_year) c ,

                   --全校各个单位各个年级房间数
                  (select b.dv_id, b.dv_name, b.stu_in_year, nvl(a.count_rm, 0) count_rm
                    from (select dv_id, stu_in_year, count(rm_id) count_rm
                            from rm
                           where rm_cat = '301'
                           group by dv_id, stu_in_year) a,
                         sc_stu_dv_year b
                   where b.dv_id = a.dv_id(+)
                     and b.stu_in_year = a.stu_in_year(+)
                   order by b.dv_id, b.dv_name, b.stu_in_year) d

                     where a.dv_id=b.dv_id
                     and a.dv_id=c.dv_id
                     and a.dv_id=d.dv_id
                     and a.stu_in_year=b.stu_in_year
                     and a.stu_in_year=c.stu_in_year
                     and a.stu_in_year=d.stu_in_year
           )
           group by dv_id,dv_name
           order by dv_id))]]>
	</queryString>
